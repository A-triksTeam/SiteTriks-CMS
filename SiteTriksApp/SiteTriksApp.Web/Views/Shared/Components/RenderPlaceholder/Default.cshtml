@model SiteTriks.Extensions.WidgetModels.MultyModel

@foreach (var widget in Model.Models)
{
    if (widget.Value.HasBaseView)
    {
        @await Html.PartialAsync($"/Views/BaseWidget/{widget.Key}/Display.cshtml", widget.Value)
    }
    else
    {
        @await Html.PartialAsync($"/Views/BaseWidget/Display.cshtml", widget.Value)
    }
}
