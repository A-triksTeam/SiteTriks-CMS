@using SiteTriks.Extentions;

@model ICollection<string>

<div id="widgets-holder" class="clearfix">
    @foreach (var item in Model)
    {
        if (WidgetRegistry.IsRegistered(item))
        {
            <div data-type="@item" class="drag">@WidgetRegistry.GetDisplayNameForType(item)</div>
        }
    }
</div>
