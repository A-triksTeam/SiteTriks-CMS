@model string

@{
    string source = $"https://www.youtube.com/embed/{Model}";
}

<div class="embeded-video">
    <iframe width="560"
            height="315"
            src="@source"
            frameborder="0"
            allowfullscreen></iframe>
    @*<iframe width="100%"
            height="610px"
            src="@source"
            frameborder="0"
            allowfullscreen></iframe>*@
</div>
