@{
    Layout = "/Views/Shared/_AccountLayout.cshtml";
}


<div class="manage-profile-container">
    @*<h2>Manage your account</h2>*@

    <div>
        <div class="row">
            <div class="col-md-2">
                @await Html.PartialAsync("_ManageNav")
            </div>
            <div class="col-md-10">
                @RenderBody()
            </div>

            @await Html.PartialAsync("_AccountFooter")
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
