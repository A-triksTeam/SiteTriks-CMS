{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "SiteTriks": {
    "DbProvider": "mssql" // Currently supports only "MSSQL" & "MySQL"
  },
  "SuperUser": {
    "Username": "#username#",
    "Password": "#password#"
  },
  "StaticFilesCaching": {
    "EnableCache": true,
    "Headers": {
      "Cache-Control-Image": "max-age=31536000",
      "Cache-Control-Css": "max-age=604800",
      "Cache-Control-Javascript": "max-age=2628000",
      "Cache-Control": "public, max-age=86000",
      "Pragma": "",
      "Expires": ""
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
